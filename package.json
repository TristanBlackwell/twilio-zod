{
  "name": "twilio-zod",
  "version": "0.0.1",
  "description": "A collection of zod helpers for Twilio",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:esm": "rollup --config ./configs/rollup.config.js",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "build:all": "yarn build:esm && yarn build:cjs"
  },
  "author": "Tristan Blackwell",
  "repository": {
    "type": "git",
    "url": "https://github.com/TristanBlackwell/twilio-zod"
  },
  "homepage": "https://github.com/TristanBlackwell/twilio-zod",
  "license": "MIT",
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "zod": "^3.23.8",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "rollup": "^2.70.1",
    "tslib": "^2.3.1",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json",
    "./locales/*": "./lib/locales/*"
  },
  "files": ["/lib", "/index.d.ts"]
}
